<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "ibatis-3-mapper.dtd">

<mapper namespace="menuRole">

		
	<insert id = "savePolicyMenuOfRole" >
		insert into role_menu_mapping 
			(menu_id, role_id)
		values
			(#{menuId}, #{roleId})
	</insert>

</mapper>
